{
    "rewrites": [
      {
        "source": "/signup-server",
        "destination": "https://asset-management-application-backend.vercel.app/signup-server"
      },
      {
        "source": "/login-server",
        "destination": "https://asset-management-application-backend.vercel.app/login-server"
      },
      {
        "source": "/create-asset-server",
        "destination": "https://asset-management-application-backend.vercel.app/create-asset-server"
      },
      {
        "source": "/api/assets-data",
        "destination": "https://asset-management-application-backend.vercel.app/api/assets-data"
      },
      {
        "source": "/delete-asset/:assetId",
